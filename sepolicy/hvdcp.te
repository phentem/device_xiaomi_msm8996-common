dontaudit hvdcp self:capability dac_override;

allow hvdcp sysfs_batteryinfo:dir search;
allow hvdcp sysfs_batteryinfo:file rw_file_perms;
allow hvdcp sysfs_power_supply:dir { open read search};
allow hvdcp sysfs_power_supply:file { open read write };
allow hvdcp sysfs_spmi:dir { open read search};
allow hvdcp sysfs_spmi:file { open read write };
